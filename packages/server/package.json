{
  "name": "server",
  "version": "1.0.0",
  "description": "server",
  "main": "dist/server.js",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=16 <17"
  },
  "private": true,
  "dependencies": {
    "argon2": "^0.28.5",
    "cors": "^2.8.5",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.3",
    "redis": "^4.1.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.6",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.41",
    "nodemon": "^2.0.16",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "typeorm": "typeorm-ts-node-commonjs -d ./src/config/db.ts",
    "migration:generate": "yarn typeorm  migration:generate -p ",
    "migration:drop": "yarn typeorm  schema:drop ",
    "migration:reset": "yarn typeorm  schema:drop  && yarn typeorm migration:run",
    "migration:up": "yarn typeorm migration:run",
    "migration:down": "yarn typeorm migration:revert"
  }
}
